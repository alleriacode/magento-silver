<script type="text/javascript">
 function changeimage()
    {
        document.getElementById('youtubeiframediv').style.display='block';
        document.getElementById('amazoncontent').innerHTML='';
    }
function changes3image()
    {
        document.getElementById('s3amazondiv').style.display='block';
        document.getElementById('amazoncontentimg').innerHTML='';
    }
function changeaudioimage()
    {
        document.getElementById('s3audioamazondiv').style.display='block';
        document.getElementById('amazonaudiocontentimg').innerHTML='';
    }
function changeebookimage()
{
    document.getElementById('s3ebookamazondiv').style.display='block';
    document.getElementById('amazonebookcontentimg').innerHTML='';
}


  
   function showdeal(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv').style.display='none';
                }
    }
    function showdeal1(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv1').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv1').style.display='none';
                }
    }
    function showdeal2(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv2').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv2').style.display='none';
                }
    }
    function showdeal3(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv3').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv3').style.display='none';
                }
    }
     function eventshowdeal(val)
    {
        if(val=='48')
            {
                document.getElementById('eventdealdiv').style.display='block';
            }
            else
                {
                    document.getElementById('eventdealdiv').style.display='none';
                }
    }
    </script>
<script type="text/javascript">
   
 jQuery(document).ready(function() {
     

jQuery('#dealprice').keypress(function() {
          {
             
              if(jQuery(this).val().length > 6)
                  {
                       jQuery("#msg6").show();  
                       jQuery("#msg6").html("You can enter only six digits");
                   }
              } 
                });
 jQuery('#description').keypress(function() {
      
        var max = 1000;
        if (jQuery(this).val().length > max) {
            var top =  jQuery('#description').scrollTop();
             jQuery('#description').val( jQuery('#description').val().substr(0, max));
             jQuery('#description').scrollTop(top);
             jQuery("#desvalidation").show();  
        jQuery("#desvalidation").html("Maximum Length Exceeded.."); 
        

        }
    });
 

jQuery("#shortdescription").keypress(function(e) {
     var max = 250;
        if (jQuery(this).val().length > max) {
            var top =  jQuery('#shortdescription').scrollTop();
             jQuery('#shortdescription').val( jQuery('#shortdescription').val().substr(0, max));
             jQuery('#shortdescription').scrollTop(top);
             jQuery("#shortdesvalidation").show();  
        jQuery("#shortdesvalidation").html("Maximum Length Exceeded.."); 
        }
  
});
});


</script>

<?php 
$login=Mage::getModel('customer/session')->isLoggedIn();
if($login):
    
$customersession=Mage::getModel('customer/session')->getCustomer();
$email=$customersession->getEmail();
//$adminusername=$customersession->getFirstname();

$user = Mage::getModel('admin/user')->getCollection()->getData();
	foreach($user as $userinfo)
	{
	  if($userinfo['email']==$email)
	  {
	     //$memberemailid=$userinfo['email'];
             $adminusername=$userinfo['username'];
	      break;
	  }
	}

$attribute = Mage::getSingleton('eav/config')->getAttribute('catalog_product', 'member_list ');
if ($attribute->usesSource()):
$options = $attribute->getSource()->getAllOptions(false);
endif;
foreach($options as $optiondata): 
    if($optiondata['label']==$adminusername):
        $productexpertname=$optiondata['value'];
    endif;
endforeach; 



