<script type="text/javascript">
    function showdeal(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv').style.display='none';
                }
    }
    function showdeal1(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv1').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv1').style.display='none';
                }
    }
    function showdeal2(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv2').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv2').style.display='none';
                }
    }
    function showdeal3(val)
    {
        if(val=='48')
            {
                document.getElementById('dealdiv3').style.display='block';
            }
            else
                {
                    document.getElementById('dealdiv3').style.display='none';
                }
    }
    </script>
<script type="text/javascript">
 jQuery(document).ready(function() {
    
       jQuery("#productbutton").click(function() {
          
            var discounttype  =jQuery('#discounttype').val();
             var discountamount  =jQuery('#discount').val();
             var productprice  =jQuery('#productprice').val();
            
              if(discounttype==1 || discounttype==4)
              {
                    var calculation = (productprice* discountamount)/100;
                    var dealprice = productprice-calculation;
                    jQuery('#dealprice').val(dealprice);
              }
              if(discounttype==2)
              {
                    var dealprice = productprice-discountamount;
                    jQuery('#dealprice').val(dealprice);
              }
              if(discounttype==3)
              {
                  var dealprice = productprice;
                    jQuery('#dealprice').val(dealprice);
              }
            
        });
        jQuery("#productbuttonaudio").click(function() {
          
            var discounttype  =jQuery('#discounttypeaudio').val();
             var discountamount  =jQuery('#discountaudio').val();
             var productprice  =jQuery('#productpriceaudio').val();
             
              if(discounttype==1 || discounttype==4)
              {
                    var calculation = (productprice* discountamount)/100;
                    var dealprice = productprice-calculation;
                    jQuery('#dealpriceaudio').val(dealprice);
              }
             if(discounttype==2)
              {
                    var dealprice = productprice-discountamount;
                    jQuery('#dealpriceaudio').val(dealprice);
              }
              if(discounttype==3)
              {
                  var dealprice = productprice;
                    jQuery('#dealpriceaudio').val(dealprice);
              }
            
        });
        jQuery("#productbuttonebook").click(function() {
          
            var discounttype  =jQuery('#discounttypeebook').val();
             var discountamount  =jQuery('#discountebook').val();
             var productprice  =jQuery('#productpriceebook').val();
             
              if(discounttype==1 || discounttype==4)
              {
                    var calculation = (productprice* discountamount)/100;
                    var dealprice = productprice-calculation;
                    jQuery('#dealpriceebook').val(dealprice);
              }
             if(discounttype==2)
              {
                    var dealprice = productprice-discountamount;
                    jQuery('#dealpriceebook').val(dealprice);
              }
              if(discounttype==3)
              {
                  var dealprice = productprice;
                    jQuery('#dealpriceebook').val(dealprice);
              }
            
        });
jQuery("#productbuttonphoto").click(function() {
          
            var discounttype  =jQuery('#discounttypephoto').val();
             var discountamount  =jQuery('#discountphoto').val();
             var productprice  =jQuery('#productpricephoto').val();
            
              if(discounttype==1 || discounttype==4)
              {
                    var calculation = (productprice* discountamount)/100;
                    var dealprice = productprice-calculation;
                    jQuery('#dealpricephoto').val(dealprice);
              }
              if(discounttype==2)
              {
                    var dealprice = productprice-discountamount;
                    jQuery('#dealpricephoto').val(dealprice);
              }
              if(discounttype==3)
              {
                  var dealprice = productprice;
                    jQuery('#dealpricephoto').val(dealprice);
              }
            
        });
      
 jQuery('#description').keypress(function() {
       // var $textarea = $(this);
        var max = 1000;
        if (jQuery(this).val().length > max) {
            var top =  jQuery('#description').scrollTop();
             jQuery('#description').val( jQuery('#description').val().substr(0, max));
             jQuery('#description').scrollTop(top);
             jQuery("#desvalidation").show();  
        jQuery("#desvalidation").html("Maximum Length Exceeded.."); 
            // alert('helo');

        }
    });
/*jQuery("#description").keypress(function(e) {
  if(e.charCode >= 48 )
    if(jQuery(this).val().length > 1000)
        jQuery("#description").removeClass('required-entry');
        jQuery("#desvalidation").show();  
        jQuery("#desvalidation").html("Maximum Length Exceeded.."); 
});*/
jQuery("#shortdescription").keypress(function(e) {
     var max = 250;
        if (jQuery(this).val().length > max) {
            var top =  jQuery('#shortdescription').scrollTop();
             jQuery('#shortdescription').val( jQuery('#shortdescription').val().substr(0, max));
             jQuery('#shortdescription').scrollTop(top);
             jQuery("#shortdesvalidation").show();  
        jQuery("#shortdesvalidation").html("Maximum Length Exceeded.."); 
        }
  
});
});



</script>

<?php 
$login=Mage::getModel('customer/session')->isLoggedIn();
if($login):
    
$customersession=Mage::getModel('customer/session')->getCustomer();
$email=$customersession->getEmail();
$adminmodel=Mage::getModel('admin/user')->getCollection()->getData();
foreach($adminmodel as $admininfo):
    if($admininfo['email']==$email)
    {
        $adminusername=$admininfo['username'];
        break;
    }
endforeach;

$attribute = Mage::getSingleton('eav/config')->getAttribute('catalog_product', 'member_list ');
if ($attribute->usesSource()):
$options = $attribute->getSource()->getAllOptions(false);
endif;
foreach($options as $optiondata): 
    if($optiondata['label']==$adminusername):
        $productexpertname=$optiondata['value'];
    endif;
endforeach; 

